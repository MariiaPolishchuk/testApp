{
  "compilerOptions": {
    "target": "es2015", // обновили target на es2015
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "downlevelIteration": true // добавили флаг downlevelIteration
  },
  "include": ["src", "src/features/Levels/Intermediate/MyFascinatingMorning"]
}
